{
    "name": "ymsoft/ttlock-sdk",
    "description": "SDK ttlock.com",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Ymsoft\\TTLockSdk\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ymsoft\\TTLockSdk\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "YarmaT",
            "email": "yarmatua@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "psr/http-client": "^1.0",
        "guzzlehttp/guzzle": "^7.8",
        "psr/cache": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.4",
        "friendsofphp/php-cs-fixer": "^3.38",
        "phpstan/phpstan": "^1.10",
        "symfony/var-dumper": "^6.4",
        "symfony/dotenv": "^6.4",
        "symfony/cache": "^6.4"
    },
    "scripts": {
        "cs-check": "vendor/bin/php-cs-fixer check",
        "cs-fix": "vendor/bin/php-cs-fixer fix",
        "phpstan": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/phpunit"
    }
}
